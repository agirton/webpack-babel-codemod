{
  "name": "webpack-babel-codemod",
  "version": "1.0.0",
  "description": "Convert common js anonymous require statements to es2015 import statements",
  "main": "index.js",
  "scripts": {
    "test": "ava"
  },
  "keywords": [],
  "author": "Adam Girton <adam.girton@me.com> (https://github.com/agirton)",
  "license": "ISC",
  "ava": {
    "files": [
      "tests/*-test.js"
    ],
    "failFast": true,
    "tap": true
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "eol-last": 2,
    "env": {
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "modules": true,
    "no-unused-vars": 2,
    "no-multiple-empty-lines": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "semi": [
      2,
      "never"
    ],
    "strict": 0,
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2
    ]
  },
  "dependencies": {
    "jscodeshift": "^0.3.13",
    "recast": "^0.11.2"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^2.2.0",
    "mocha": "^2.4.5"
  }
}
